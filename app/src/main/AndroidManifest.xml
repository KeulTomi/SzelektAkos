<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.szelektakos">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".SzelektAkos"
        android:allowBackup="true"
        android:icon="@mipmap/launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mainscreen.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".shop.ShopActivity" />
        <activity android:name=".THG_Web" />
        <activity android:name=".games.PickOneGame" />
        <activity android:name=".games.TrueFalseGame" />
        <activity android:name=".games.JumpGameActivity" />
    </application>

</manifest>